<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="br.ufrn.imd.circusmanager.Controller.TelaZooController">
    <Label  text="ZOO" style="-fx-font-size: 24px; -fx-font-weight: bold;" />
    <Button text="Ver lista de animais" onAction="#verListaAnimais"/>
    <Button text="Adicionar animal" onAction="#adicionarAnimal"/>
    <Button text="Remover animal" onAction="#removerAnimal"/>
    <Button text="Voltar" onAction="#voltar"/>
</VBox>
